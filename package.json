{
  "name": "dalmong-logic-hunter-v2",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "nodemon",
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "build": "tsc --build && vite build",
    "preview": "vite preview",
    "start": "node dist/backend/server.js"
  },
  "nodemonConfig": {
    "watch": [
      "backend/"
    ],
    "ext": "ts,json",
    "exec": "node --loader ts-node/esm backend/server.ts"
  },
  "dependencies": {
    "@google/genai": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  }
}